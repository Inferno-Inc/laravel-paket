{
    "name": "cybercog/laravel-paket",
    "description": "Personal Package Manager.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "laravel",
        "composer",
        "packagist",
        "package",
        "manager",
        "management",
        "repository",
        "packet",
        "paket",
        "dependency",
        "artifact",
        "library",
        "development"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/cybercog/laravel-paket",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-paket/issues",
        "wiki": "https://github.com/cybercog/laravel-paket/wiki",
        "source": "https://github.com/cybercog/laravel-paket",
        "docs": "https://laravel-paket.readme.io"
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/database": "5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "mcstreetguy/composer-parser": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "mockery/mockery": "^1.0",
        "orchestra/database": "~3.6.0|~3.7.0|~3.8.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Contracts\\Paket\\": "contracts/",
            "Cog\\Laravel\\Paket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Tests\\Laravel\\Paket\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Laravel\\Paket\\PaketServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
